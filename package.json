{
  "name": "blog",
  "version": "1.0.0",
  "description": "my blog",
  "main": "src/main.js",
  "scripts": {
    "beautiful": "lint-staged",
    "start": "nodemon --inspect src/main.js",
    "watch": "node-sass --watch src/resources/scss/ --output src/public/css/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Baohamchoi/project-nodejs.git"
  },
  "keywords": [
    "blog",
    "nodejs",
    "nodejs"
  ],
  "author": "baobao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Baohamchoi/project-nodejs/issues"
  },
  "homepage": "https://github.com/Baohamchoi/project-nodejs#readme",
  "lint-staged": {
    "src/**/*.{js, json, scss}":
      "prettier --single-quote --trailing-comma all --tab-width 4 --write"
  },
  "husky": {
